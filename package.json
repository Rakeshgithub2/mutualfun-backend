{
  "name": "mutual-funds-backend",
  "version": "1.0.0",
  "description": "Production-ready backend for mutual fund platform",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon src/app.js",
    "dev:simple": "tsx src/server-simple.ts",
    "dev:direct": "tsx watch src/index.ts",
    "dev:old": "tsx watch src/index.ts",
    "dev:local": "dotenv -e .env.local -- tsx watch src/index.ts",
    "postinstall": "prisma generate",
    "build": "prisma generate && tsc",
    "vercel-build": "prisma generate && tsc",
    "start": "node src/app.js",
    "worker": "node dist/worker.js",
    "worker:dev": "tsx watch src/worker.ts",
    "worker:local": "dotenv -e .env.local -- tsx watch src/worker.ts",
    "scheduler": "tsx src/scripts/start-scheduler.ts",
    "scheduler:dev": "tsx watch src/scripts/start-scheduler.ts",
    "scheduler:local": "dotenv -e .env.local -- tsx watch src/scripts/start-scheduler.ts",
    "db:seed": "tsx src/db/seed-mongodb.ts",
    "db:seed:local": "dotenv -e .env.local -- tsx src/db/seed-mongodb.ts",
    "seed:4000-funds": "node src/scripts/seed-4000-funds.js",
    "seed:market": "node src/scripts/seed-market-indices.js",
    "setup:local": "tsx src/scripts/setup-local-mongodb.ts",
    "setup:15k": "node scripts/master-setup.js",
    "ingest:funds": "node scripts/ingestion-engine.js",
    "verify:15k": "node scripts/verify-funds.js",
    "verify:master": "node scripts/verify-master-system.js",
    "monitor:system": "node scripts/monitor-system.js",
    "indexes:add": "node scripts/add-indexes.js",
    "update:nav": "node jobs/update-nav.job.js",
    "update:returns": "node jobs/update-returns.job.js",
    "update:news": "node jobs/update-news.job.js",
    "update:indices": "node jobs/update-indices.job.js",
    "verify:indices": "node scripts/verify-all-indices.js",
    "import:funds": "tsx src/scripts/import-funds.ts funds",
    "import:real-world": "tsx src/scripts/import-real-world-funds.ts",
    "import:real-world:equity": "tsx src/scripts/import-real-world-funds.ts --equity=150 --commodity=30",
    "import:real-world:commodity": "tsx src/scripts/import-real-world-funds.ts --equity=50 --commodity=100",
    "import:real-world:comprehensive": "tsx src/scripts/import-real-world-comprehensive-funds.ts",
    "import:comprehensive-amfi": "tsx src/scripts/import-comprehensive-amfi.ts",
    "import:comprehensive-amfi:test": "tsx src/scripts/import-comprehensive-amfi.ts --limit=100",
    "import:prices": "tsx src/scripts/import-funds.ts prices",
    "import:update-navs": "tsx src/scripts/import-funds.ts update-navs",
    "import:all": "tsx src/scripts/import-funds.ts all",
    "worker:sectors": "tsx src/workers/sector-allocation-worker.ts",
    "worker:sectors:stats": "tsx src/workers/sector-allocation-worker.ts -- --stats",
    "verify:backend": "tsx src/scripts/verify-backend-fixes.ts",
    "test:redis": "tsx src/scripts/test-redis.ts",
    "migrate:professional": "node scripts/migrate-to-professional-architecture.js",
    "migrate:professional:force": "node scripts/migrate-to-professional-architecture.js --force",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mutual-funds",
    "backend",
    "api",
    "typescript",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "5.22.0",
    "@types/nodemailer": "^7.0.4",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.15.4",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "cron": "^3.5.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-slow-down": "^3.0.1",
    "google-auth-library": "^10.5.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.48",
    "mongodb": "^6.21.0",
    "mongoose": "^9.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "prisma": "5.22.0",
    "resend": "^2.1.0",
    "socket.io": "^4.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.24",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vercel/node": "^2.3.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
